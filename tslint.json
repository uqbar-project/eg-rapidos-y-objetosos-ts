{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "semicolon": [
      false,
      "always"
    ],
    "quotemark": [
      true,
      "single"
    ],
    "max-line-length": false,
    "no-console": false,
    "interface-name": false,
    "no-any": true,
    "max-classes-per-file": false
  },
  "rulesDirectory": []
}